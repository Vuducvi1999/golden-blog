$('#wrap-unpublished-component').html(
`<% unpublished_paginate(@unpublished).each do |post| %>
  <div class="border ps-3">
    <%= j render partial:'shared/post_dashboard_item', locals: { post: post, is_admin_manage: true } %>
  </div>
<%end %>`
);
$('#call-paginate-unpublished-ajax').html(
`<%= j will_paginate unpublished_paginate(@unpublished), inner_window: 2, outer_window: 1,\
:renderer => RemoteLinkPaginationHelper::LinkRenderer, param_name: "page_unpublished" %>`
);
